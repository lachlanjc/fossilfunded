{
  "name": "fossilfunded",
  "version": "2.0.0-1",
  "description": "Website to analyze fossil fuel money in Congress.",
  "private": true,
  "scripts": {
    "dev": "x0 dev src/App.js -op 8080",
    "build": "x0 build src/App.js --out-dir docs",
    "start": "serve docs",
    "download": "node data/download.js",
    "parse": "node data/parse.js",
    "analysis": "node data/analysis.js",
    "data": "npm run download && npm run parse && npm run analysis",
    "icon": "repng src/Icon.js -w 512 -h 512 -o docs -f icon"
  },
  "devDependencies": {
    "@compositor/x0": "^1.0.9",
    "axios": "^0.16.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "comma-number": "^2.0.0",
    "cxs": "^6.0.0",
    "flow-bin": "^0.47.0",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rebass": "^1.0.4",
    "repng": "^2.0.0-alpha.1",
    "respin": "^1.0.1",
    "scrape-it": "^3.3.2",
    "unfetch": "^3.0.0",
    "unformat-usd": "^0.1.1"
  },
  "dependencies": {
    "serve": "^6.1.0"
  },
  "author": "Lachlan Campbell <lachlan.campbell@icloud.com>",
